{
    "name": "iarafat/phpqrcode",
    "description": "PHP8.0+ QR Code generator library",
    "keywords": [
        "iarafat",
        "phpqrcode"
    ],
    "homepage": "https://github.com/iarafat/phpqrcode",
    "license": "MIT",
    "authors": [
        {
            "name": "MD Iyasin Arafat",
            "email": "miyasinarafat@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Iarafat\\QRCode\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iarafat\\QRCode\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
